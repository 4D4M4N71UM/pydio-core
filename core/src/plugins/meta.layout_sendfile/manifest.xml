<?xml version="1.0" encoding="UTF-8"?>
<ajxp_plugin label="Send File Layout" description="Simple template for quick upload'n'share operation"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file:../core.ajaxplorer/ajxp_registry.xsd"
             id="meta.layout_sendfile">
    <client_settings>
        <resources>
            <js className="SendFile" file="plugins/meta.layout_sendfile/res/build/SendFile.js"/>
        </resources>
    </client_settings>
    <class_definition classname="Pydio\Access\Meta\Layout\LayoutSendFile" filename="plugins/meta.layout_sendfile/LayoutSendFile.php"/>
    <registry_contributions>
        <client_configs>
            <template_part
                    name="orbit_content"
                    ajxpId="orbit_content"
                    theme="material"
                    namespace="SendFile"
                    component="Template"
                    dependencies="PydioComponents,PydioTasks"
                    props="{}"/>
        </client_configs>
    </registry_contributions>
</ajxp_plugin>
