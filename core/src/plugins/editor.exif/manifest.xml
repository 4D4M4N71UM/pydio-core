<?xml version="1.0" encoding="UTF-8"?>
<editor id="editor.exif" openable="true" text="editor.exif.1" iconClass="icon-list-ul" title="editor.exif.2"
        className="PydioExif.Editor" mimes="jpg,jpeg"
        formId="exif_box" label="CONF_MESSAGE[EXIF Extractor]"
        description="CONF_MESSAGE[Display the Exif metadata contained inside the JPG files. Can locate the image on a map if it contains geolocation data.]"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file:../core.ajaxplorer/ajxp_registry.xsd">
    <client_settings>
        <resources>
            <js file="plugins/editor.exif/res/build/PydioExif.js" className="PydioExif"/>
            <i18n namespace="editor.exif" path="plugins/editor.exif/res/i18n" />
        </resources>
    </client_settings>
	<dependencies>
		<!-- Stream Wrapper Access -->
		<activePlugin pluginName="meta.exif|image.diaporama"/>
	</dependencies>
</editor>
