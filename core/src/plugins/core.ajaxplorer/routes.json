{
  "/api": {
    "methods": "*",
    "routes": [
      "/api[{optional:.+}]"
    ],
    "class": "Pydio\\Core\\Http\\Base",
    "method": "handleRoute"
  },
  "/wopi": {
    "methods": "*",
    "routes": [
      "/wopi[{optional:.+}]"
    ],
    "class": "Pydio\\Core\\Http\\Base",
    "method": "handleRoute"
  },
  "/user":{
    "methods": "*",
    "routes": [
      "/user[{optional:.+}]"
    ],
    "class": "Pydio\\Core\\Http\\Base",
    "method": "handleRoute"
  },
  "/shares": {
    "methods": "*",
    "routes": [
      "/shares[{optional:.+}]"
    ],
    "class": "Pydio\\Core\\Http\\Dav\\DAVServer",
    "method": "handleRoute"
  },
  "/ocs-provider":{
    "methods": "*",
    "routes": [
      "/ocs-provider[{optional:.+}]"
    ],
    "class": "Pydio\\OCS\\OCSPlugin",
    "method": "startServer"
  },
  "/ocs":{
    "methods": "*",
    "routes": [
      "/ocs[{optional:.+}]"
    ],
    "class": "Pydio\\OCS\\OCSPlugin",
    "method": "startServer"
  },
  "/public":{
    "methods": "*",
    "routes": [
      "/public/{hash}/dl/[{optional:.+}]",
      "/public/{hash}",
      "/public[{optional:.+}]"
    ],
    "class": "Pydio\\Share\\ShareCenter",
    "path" : "plugins/action.share/src/ShareCenter.php",
    "method": "publicRoute"
  },
  "/favicon":{
    "methods": "GET",
    "routes":[
      "/favicon.ico"
    ],
    "class":"Pydio\\Core\\Http\\Base",
    "method":"handleRoute"
  },
  "/": {
    "methods": "*",
    "routes": [
      "/",
      "/index[{optional:.+}]",
      "/dashboard[{optional:.+}]",
      "/settings[{optional:.+}]",
      "/welcome[{optional:.+}]",
      "/ws-[{optional:.+}]"
    ],
    "class": "Pydio\\Core\\Http\\Base",
    "method": "handleRoute"
  }
}